.ribbon_header{
 display:block; 
 height: 69px;
 vertical-align: top;
 z-index: 100;

}
#recorder{
  float:left;
}

.shadow_cover{
	position:relative;
	background:#ba3a9d;
	float:left;
  display:inline;
  height: 69px;
  width: 20px;
  line-height: 69px;
  z-index: 1000; 
}

.ribbon{
  vertical-align: middle;
	position:relative;
	color:#fff;
	background:#ba3a9d;
	font-size:1.5em;
	float:left;
  padding-left: 20px;
  padding-right: 10px;
	margin-left:-10px;
	margin-bottom:20px;
	text-shadow:0 -1px #8e156d;
  display:inline;
  height: 69px;
  line-height: 69px;
  font-family:"CTMercuriusStd-MediumItalic";
  font-size: 30px;
  color: #f7c5e9;
  z-index: 100;

  -moz-box-shadow:12px 5px 10px #ABABAB;
  -webkit-box-shadow:12px 5px 10px #ABABAB;
  -o-box-shadow:12px 5px 10px #ABABAB;
  box-shadow:12px 5px 10px #ABABAB;
}
.ribbon:before{
	content:" ";
	background: url("/images/ribbon.png");
	display:inline;
	width:10px;
	height:10px;
	position:absolute;
	bottom:0;
	left:0;
	float: left;
	margin-bottom:-10px;
  z-index:1;
  font-family:"CTMercuriusStd-MediumItalic";
}
.tail{
  height:69px;
  width:35px;
  float:left;
  background: url("/images/tail.png") no-repeat;
  z-index: 99;
  -moz-box-shadow:-12px 5px 10px #ABABAB;
  -webkit-box-shadow:-12px 5px 10px #ABABAB;
  -o-box-shadow:-12px 5px 10px #ABABAB;
  box-shadow:-12px 5px 10px #ABABAB;
  margin-bottom: 20px;
  position: relative;


}

#message_area{
  height: 40px;
  margin-bottom: 10px;
}

#flash_message{
  text-align: center;
  width: 400px;
  background-color: #45beee;
  font-family: DINConBol;
  font-size: 20px;
  color: #5f5f5f;
  margin-bottom: 20px;
  padding-top: 10px;
  padding-bottom: 10px;
  margin-left: -30px;
  @include border-radius(5px,5px,5px,5px);
  visibility: hidden;
}

#paginator{
  text-align: center;
  margin-top: 50px;
  font-family: Arial;
}

#flash_warning{
  text-align: center;
  width: 400px;
  background-color: #fdf698;
  font-family: DINConBol;
  font-size: 20px;
  color: #5f5f5f;
  margin-bottom: 20px;
  padding-top: 10px;
  padding-bottom: 10px;
  margin-left: -30px;
  @include border-radius(5px,5px,5px,5px);
  visibility: hidden;  
}


.video_info{
  float: left;
  width: 375px;
  margin-left: 41px;
  margin-top: 12px;
  display: inline;
  a {
    color: #1f9fd1;
    text-decoration: none;
  }
}

#videos_container{
  height: 250px;
}        
           
.video_twitter_text{
 font-family: Georgia;
 font-size: 13px;
 color: #9e3f84; 
}
.video_age{ 
  font-family: Arial;
  font-size: 10px;
  color: #5b5b5b;
  margin-bottom: 15px;
}

.video_thumbnail{
  float: left;
  display: inline;
  margin-right: 10px;

  
  img {
    border: 5px solid #cdcdcd;
    height: 85px;
    width: 114px;
  }
}

.video_stats{
  vertical-align: text-bottom;
  font-family: Arial;
  font-size: 10px;
  color: #000000;
}



/* Tweet text form*/
form {
  display: inline;
}


a {
  color: #949494;
  text-decoration: none;
}
#tweet_extra{
  width: 350px;
}
#tweet_prompt{
  text-align: center;
  width: 350px;
  font-family: DINCond;
  font-size: 20px;
  margin-bottom: 10px;
  .light_text{
   color: #949494; 
  }
}

#follow_checkbox{
  font-family: Arial;
  font-size: 10px;
  display: inline;
  float: left;

}

#framey_post{
  margin-bottom: 150px;
}

.pagination{
  text-align: center;
  font-family: "DINConBol";
  font-size: 20px;
  
  
  em{
   color:#fff; 
   width: 27px;
   height: 32px;
   background: url("/images/current_page_bg.png") no-repeat;
   text-align: center;
   padding: 10px;
  }
  
  .previous_page{
    color: #a5a5a5;
    margin-left:10px;
    margin-right:10px;
  }

  .next_page{
    color: #a5a5a5;
    margin-left:10px;
    margin-right:10px;
  }
  
  a{
    margin-left:10px;
    margin-right:10px;
    color: #000;
    .previous_page{
      color: #a5a5a5;
    }

    .next_page{
      color: #a5a5a5;
    }
  }
}


body {
  margin: 0;
  padding: 0;
}

a img{
 border: 0px; 
}

.header {

  width: 100%;
  height: 128px;
  border-bottom: 6px solid rgb(235,235,235);
  background: url("/images/header.png") repeat-x;
  
  .center {
    width: 940px;
    margin-left: auto;
    margin-right: auto;
    
    .logo {
/*      width: 230px;
      height: 128px;*/
/*      background: url("/images/logo.png") no-repeat;*/
      float: left;
      margin-top: 30px;
      margin-left: 30px;
    }
    
    .loginWrapper {
      color: white;
      font-family: "CTMercuriusStd-MediumItalic";
      font-size: 19px;
      float: right;
      width: 230px;
      margin-right: 95px;
      margin-top: 49px;
      text-align: right;
      
      a {
        color: white;
        text-decoration: none;
      }
      
      a:hover {
        text-decoration: underline;
      }
    }
  }
}

.mainContent {
  
  width: 100%;
  height: 984px;
  background: url("/images/bg.jpg") repeat-x;
  
  .clear{
    clear: both;
  }

  .counter{
    float: right;
    font-family: Arial;
    font-size: 10px;
    color: #a6a6a6;
  }
  
  .twitter_text_area{
    vertical-align: top;
    padding: 10px;
    border: 0px;
    margin-right: 10px;
    margin-bottom: 10px;
    width: 350px;
    height: 65px;
    -moz-box-shadow: 0px 0px 7px #45beee; /* Firefox 3.6 and earlier */
    -webkit-box-shadow: 0px 0px 7px #45beee; /* Safari */
    box-shadow: 0px 0px 7px #45beee;
    font-family: DINConBol;
    font-size: 15px;
    color: #5f5f5f; 
    display: inline;
    float: left;
    
  }
  
  input[type="checkbox"]{
/*    padding:10px;
    margin:5px;
    cursor:pointer;
    color:#ffffff;
    -moz-border-radius: 0px; 
    -webkit-border-radius: 0px;
*/  
    width: 16px;
    height: 16px;
    background: url("/images/checkbox.png") no-repeat;
    display: block;
    clear: left;
    float: left;
  }
  
  .center {
    width: 940px;
    height: 955px;
    background: url("/images/fritter_words_in_bg.png") no-repeat bottom white;
    margin-left: auto;
    margin-right: auto;
    margin-top: -2px;
    
    .num {
      font-family: Arial;
      font-size: 12px;
      color: black;
      float: left;
      
    }
    

    .tweet_button{
      float: left;
      display: inline;
      margin-bottom: 10px;
      
      a.tweet_button_link{
        text-decoration: none;
        font-family: Arial;
        font-size: 10px;
        color: #45beee;
      }
      
      .tweet_button_text{
        margin-left: 20px;
        font-family: Arial;
        font-size: 10px;
        color: #45beee;
      }
    }
    .heart {
      float: left;
      width: 18px;
      height: 17px;
      background: url("/images/heart.png");
    }
    
    .when {
      font-family: Arial;
      padding-left: 8px;
      float: left;
      width: 190px;
      margin-top: 7px;
      color: black;
      font-size: 12px;
    }    
    
    .bottom {
      height: 274px;
      
      #tweet_comment{
        margin-left: 230px;
        height: 150px;
        form{
          display: inline;
        }
        
        #comment_extra{
          width: 350px;
          #counter{
            float: right;
            display: inline;
          }
        }

        #user_name{
          font-family: "DINConBol";
          font-size: 20px;
          color: #1f9fd1;
          display: inline;
        }
        
        #comment_area{
          #text_area{
            position: relative;
            top-margin: -20px;
            vertical-align: top;
          }

        }
        
        #disclaimer_text{
          font-family: Arial;
          font-size: 10px;
          color: #000000;
          display: block;
        }
        
      }
      
      .popular_title {
        font-family: "DINCond";
        font-size: 19px;
        font-weight: bold;
        width: 200px;
        padding-left: 41px;
        color: #3f3f3f;
        
      }
      
      .popular_divider {
        width: 863px;
        height: 8px;
        margin-top: 8px;
        background-color: #dadada;
        margin-left: 41px;
        @include border-radius(2px, 2px, 2px, 2px);
        
      }
      
      .popular_video {
        float: left;
        width: 350px;
        height: 103px;
        margin-left: 41px;
        margin-top: 12px;
        
        .thumbnail {
          float: left;
          width: 134px;
          height: 93px;
          overflow:hidden;
          border: 2px solid #cdcdcd;
          
          img {
            width: 134px;
          }
        }
        
        .title {
          float: left;
          padding-left: 8px;
          width: 190px;
          font-family: Georgia;
          font-size: 20px;
          color: #9e3f84;
        
        }
        .by {
          font-family: Arial;
          float: left;
          padding-left: 8px;
          width: 190px;
          margin-top: 3px;
          color: #5b5b5b;
          font-size: 15px;
          
          a {
            color: #058ec5;
            text-decoration: none;
          }
          
          a:hover {
            text-decoration: underline;
          }
        }

        .views {
          float: left;
          width: 190px;
          padding-left: 8px;
          margin-top: 15px;
          
        }
      }

    }
    
    .top { 
      width: 100%;

      padding-top: 20px;
      #home_page{
        padding-top: 60px;
      }
      
      #user_videos{
        #user_info{
          margin-right: 20px; 
          margin-left: 20px;
          height: 50px;
          
          #user_image{
            display:inline;
            float: left;
            margin-right: 5px;
          }
        
          #user_name{
            font-family: Arial;
            font-size: 15px;
            color: #1f9fd1;

            a {
              color: #1f9fd1;
              text-decoration: none;
            }
          }
          
          #user_stats{
            margin-top: 20px;
            font-family: Arial;
            font-size: 10px;
            color: #000000;
          }
          
        }
        
        #videos_display{
          
          filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f3f3f3', endColorstr='#ffffff'); /* for IE */
          background: -webkit-gradient(linear, left top, left bottom, from(#f3f3f3), to(#fff)); /* for webkit browsers */
          background: -moz-linear-gradient(top,  #f3f3f3,  #fff); /* for firefox 3.6+ */
          padding-top:20px;
          padding-left: 20px;
          padding-right: 20px;
          
          .video_list_header{
            background-color: #a9e7ff;
            color: #0b5572;
            display: block;

            font-family: "DINConMed";
            font-size: 24px;

            padding-top: 10px;
            padding-bottom: 10px;
            padding-left: 10px;

          }                    
          
        }
        
        
      }
      
      #video_show{
        #player_wrapper{
          filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f3f3f3', endColorstr='#ffffff'); /* for IE */
          background: -webkit-gradient(linear, left top, left bottom, from(#f3f3f3), to(#fff)); /* for webkit browsers */
          background: -moz-linear-gradient(top,  #f3f3f3,  #fff); /* for firefox 3.6+ */
          padding-top: 40px; 
          padding-bottom: 40px;
          padding-left: 290px;
          
          
          #tweet_form{
            margin-left: -45px;
          }
          
        }
        
        #processing_video{
          text-align: center;
          font-family: Georgia;
          font-size: 20px;
          color: #9e3f84;
          padding-top: 150px;
          filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#f3f3f3', endColorstr='#ffffff'); /* for IE */
          background: -webkit-gradient(linear, left top, left bottom, from(#f3f3f3), to(#fff)); /* for webkit browsers */
          background: -moz-linear-gradient(top,  #f3f3f3,  #fff); /* for firefox 3.6+ */
          
        }
        #display_twitter_text{
          font-family: Georgia;
          font-size: 20px;
          color: #9e3f84;
          text-align: center;
          margin-left: -280px;
          margin-bottom: 20px;

        }
        
        #video_info{

          margin-right:50px;          
          #user_image{
            float: left;
            display: inline;
            height: auto;
            margin-left: 20px;
            margin-right: 10px;
            margin-bottom: 10px;
            img {
              border: 5px solid #cdcdcd;
            }
            
          }
          
          #video_age{
            font-family: Arial;
            font-size: 10px;
            color: #000000; 
            margin-bottom: 15px;
          }
          
          #video_title{
           font-family: Georgia;
           font-size: 15px;
           color: #9e3f84; 
          }
          
          #video_byline{
           font-family: Arial;
           font-size: 11px;
           color: #5b5b5b;
           
           a {
             color: #058ec5;
             text-decoration: none;
           }
          }
          
          
          #video_likes{
            font-family: Arial;
            font-size: 12px;
            color: #000000;
            a {
              text-decoration: none;
            }
          }
          
          #video_screen{
            float: right;
            margin-top: 0px;
            display: inline;
            margin-right: 10px;
          }
        }
      }
      
      .left {
        float: left;
        width: 333px;
        height: 445px;
        position: relative;
        
        .camera{
          margin-bottom: -6px;
          z-index: 1;
          position: relative;
        }
      }
      
      #user_show{
        
        #framey_content{
         margin-top: 45px; 
        }
        
        margin-left: 290px;
        #recorder_post_container{
          height: 400px;
        } 
                       
        #framey_recorder{
          position: relative;
/*          height: 400px;*/
          overflow: hidden;
        }
        
        #instructions{
          text-align: left;
          margin-left: -75px;
        	font-size:30px;
          font-family:"CTMercuriusStd-MediumItalic";
          
          .number{
            color: #a2a2a2;
            display: inline;
          }
          
          .pink_text{
            color:#cc53ab;  
            display: inline;
/*            cc53ab*/
          }
          
          .dark_text{
            color:#505050;
            display: inline;
          }
          
        }
        
      }
      
      .right {
        width: 560px;
        float: right;
        position: relative;
      
        .easily_record {
          text-shadow: #ABABAB 0px 0px 5px;
          font-family: "CTMercuriusStd-MediumItalic";
          font-size: 60px;
          color: #cc53ab;
          width: 100%;
          width: 475px;
          position: relative;
          line-height: 51px;
          
          
          .twitter_bird {
            width: 41px;
            height: 26px;
            background: url("/images/twitter_bird.png") no-repeat;
            position: absolute;
            right: 82px;
            bottom: 15px;
          }
        }
        .simplest_way  {
          width: 100%;
          margin-top: 5px;
          font-family: "DINConLig";
          font-size: 25px;
          color: #949494;
          
          span {
            color: black;
          }
          
          a {
            color: #199acd;
            text-decoration: none;
          }
          
          a:hover {
            text-decoration: underline;
          }
        }
        .get_started {
          width: 100%;
          margin-top: 66px;
          font-family: "DINConBol";
          font-size: 25px;
          font-weight: bold;
          
        }
        .dont_forget {
          width: 100%;
          font-family: "DINCond";
          font-size: 15px;
          font-weight: bold;
          color: #9d9d9d;
        }

        .sign_up_button {
          margin-top: 36px;
          cursor: pointer;
          width: 445px;
          height: 141px;
          a {background: url("/images/twitter_button.png") no-repeat;}
          background: url("/images/twitter_button.png") no-repeat;
          
        }
        
        
        .sign_up_button:hover {
          margin-top: 36px;
          cursor: pointer;
          width: 445px;
          height: 141px;
          background: url("/images/inverted_twitter_button.png") no-repeat;
        }
        
      }
    }
  }
